<Window x:Class="WCFTestingTool.AddService"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Service end point" Height="100" Width="500"
        ShowInTaskbar="False"
        ResizeMode="NoResize"
       
        
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txtEndPoint" Height="22" Grid.Column="0" Grid.Row="0"/>
        <StackPanel Grid.Column="0" Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button x:Name="btnOk" Click="btnOk_Click" Content="Ok" Width="55"/>
            <Button x:Name="btnCancel" Click="btnCancel_Click" Content="Cancel" Width="55"/>
            
        </StackPanel>
    </Grid>
</Window>
